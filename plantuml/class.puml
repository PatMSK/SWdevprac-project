@startuml

class User {
    - name: String
    - email: String
    - password: String
    - telephone: Number
    - role: String
    - resetPasswordToken: String
    - resetPasswordExpire: Date
    - createdAt: Date
    + getSignedJwtToken()
    + matchPassword()
}

class Dentist {
    - name: String
    - yearsOfExperience: Number
    - expertise: String
}

class Appointment {
    - apptDate: Date
    - createAt: Date
    + user: ObjectId â†’ User
    + dentist: ObjectId â†’ Dentist
}


